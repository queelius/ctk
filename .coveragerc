[run]
source = ctk
omit =
    */cli_chat.py
    */cli_tag.py
    */interfaces/*
    */taggers/*
    */base_importer.py
    */utils.py
    */sanitizer.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
